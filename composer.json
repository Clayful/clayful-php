{
    "name": "clayful/clayful",
    "description": "PHP SDK for Clayful API",
    "version": "2.5.0",
    "type": "library",
    "keywords": [
        "eCommerce",
        "clayful"
    ],
    "homepage": "https://github.com/Clayful/clayful-php",
    "readme": "./README.md",
    "license": "MIT",
    "authors": [
        {
            "name": "Daeik Kim",
            "email": "daeik.kim@clayful.io"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "~6.0|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.5.*"
    },
    "autoload": {
        "psr-4": {
            "Clayful\\": [
                "src/",
                "src/Models/"
            ]
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Clayful\\ServiceProvider"
            ],
            "aliases": {
                "ClayfulFacade": "Clayful\\Facade"
            },
            "dont-discover": [
                "*"
            ]
        }
    }
}
