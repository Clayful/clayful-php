{
    "name": "clayful/clayful",
    "description": "PHP SDK for Clayful API",
    "version": "2.4.1",
    "type": "library",
    "keywords": ["eCommerce", "clayful"],
    "homepage": "https://github.com/Clayful/clayful-php",
    "readme": "./README.md",
    "license": "MIT",
    "authors": [
        {
            "name": "Daeik Kim",
            "email": "daeik.kim@clayful.io"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "~6.0|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.5.*"
    },
    "autoload": {
        "psr-4": {
            "Clayful\\": ["src/", "src/Models/"],
            "Clayful\\Laravel": ["src/Laravel"]
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Clayful\\Laravel\\ServiceProvider"
            ],
            "aliases": {
                "Clayful": "Clayful\\Laravel\\Facade\\ClayfulFacade"
            },
            "dont-discover": [
                "*"
            ]
        }
    }
}
